{
  "name": "react-interactive-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@modulz/radix-icons": "^3.3.0",
    "@stitches/react": "^0.0.3-canary.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-interactive": "file:..",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "browserslist-config-css-grid": "^1.0.0",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.5.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "dev": "npm install --save ../ && npm start && npm run devCleanup",
    "devCleanup": "",
    "deploy": "gh-pages --dist build --message Built-`date +%Y%m%d`-`date +%H%M%S`",
    "predeploy": "npm run devCleanup && npm run lint && npm test -- --watchAll=false && npm run build",
    "lint": "eslint src",
    "start": "trap 'exit 0' SIGINT; react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 versions or > 0.2% and not dead and extends browserslist-config-css-grid"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
